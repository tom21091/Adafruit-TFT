/******************************************************************************
 * Function:        void setBitmap(const uint8_t *array, int x, int y, int w, int h)
 *
 * Description:		This function copies a bitmap byte array to the display map to be displayed
 *                        This method of displaying smaller bitmaps is not as efficient,
 *                        since it needs to update the entire screen each time
 * Inputs:			const uint8_t *array - pointer to the byte array
 *                              int x - horizontal offset
 *                              int w - width of bitmap
 *                              int h - height of bitmap
 *                              
 * Output:          none
 *
 * Example:		drawMonoByteArray(HEIGHT/2, WIDTH/2, array, 32, 32, 0xABCD, false);
 *
 *****************************************************************************/
void setBitmap(const uint8_t *array, int x, int w, int h){
 if (x<(WIDTH-w-1)){
  for (int a=0; a<(HEIGHT*WIDTH/8); a++)// Clear whole display map first
  {
    displayMap[a]=0;
  }
  
    for (int i=0; i<h; i++)//Whole rows
    {
      for (int j= x; j<(x+w); j++)//Offset columns
      {
         displayMap[(i)*WIDTH+j]=array[i*w+j-x];//Sets bytes in the displayMap array, aligned correctly
      }
      
    }
  }
}
/******************************************************************************
 * Function:        void drawMonoByteArray
 *
 * Description:		This function writes a monochromatic bitmap's byte array to memory on the Adafruit TFT
 *
 * Inputs:			int x - Horizontal offset
 *				int y - Vertical offset
 *                              uint8_t * array - Pointer to the byte array
 *                              int w - width of the bitmap
 *                              int h - height of the bitmap
 *                              uint16_t color - Base color
 *                              bool fade - Enables fade to white horizontally (Color parameter must be bitwise masked with &~0xA294 to look right
 * Output:          none
 *
 * Example:		drawMonoByteArray(HEIGHT/2, WIDTH/2, array, 32, 32, 0xABCD, false);
 *
 *****************************************************************************/
void drawMonoByteArray(int x, int y, const uint8_t * array, int w, int h, uint16_t color, bool fade, int delta){

uint16_t cbuff;//Color Buffer

/*
//This would be a better method of adjusting colors, but I had already finished implementing my function by the time I realised it
//The TFT (in its current configuration) recieves data in the form of 16 bits of color data:
//0bRRRRRGGGGGGBBBBB
//These can be masked like so:

uint8_t red, green, blue;
red=(color>>11)&0b11111;
green=(color>>5)&0b111111;
blue=color&0b11111;

//This data can then be stored into the color buffer like so
cbuff|=red<<11;
cbuff|=green<<5;
cbuff|=blue;
//(I haven't tested this but it should work)
*/

//This function took a while to get right, so sorry that it's a bit ambiguous

for (int i=0; i<((h+7)/8); i++)//Vertical byte counter (h bits, but a byte contains 8 bits, so divide by 8 to get number of bytes)
  {
    if (fade==true)//If fade enabled
      cbuff=color+i*2081;//(adds 0b0000100000100001 to increment each color in the two bytes by 1 each vertical byte
    else
      cbuff=color;//Else, monochromatic color
    for (int j=0; j<w; j++)//Horizontal counter (width of the bitmap)
    {
      for (int k=0; k<8; k++)//Byte counter (reads each bit in the byte)
      {
        if (array[i*w+j]&(1<<k))//If a bit within the byte in the byte array is 1
        {
          drawPixel(x+j, 8*i+k, cbuff);//Draw the pixel on the screen. x is offset, j is horizontal distance, i is vertical byte number, k is bit in the byte
        }
        else{
          drawPixel(x+j, 8*i+k, WHITE);//Else draw white
        }
      }
    }
    //Gets rid of old image on screen
  if (delta>w){//If last image was far away (further than the width of the picture)
    fillRect(x-delta, 8*i, w, 8, WHITE);
  }
  else if (delta>0)//If last image was close, clear only necessary pixels
    fillRect(x-delta, 8*i, delta, 8, WHITE);
  else if (-delta>w)//If last image was far and on opposite side
    fillRect(x-delta, 8*i, w, 8, WHITE);
  else//If last image was close on opposite side
    fillRect(x+w, 8*i, -delta, 8, WHITE);
  }
} 


      
      

